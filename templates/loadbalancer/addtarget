# loadbalancer/addtarget

cat << EOF
{
    "loadbalancerid": "$loadbalancerid",
    "backendname": "$backendname",
    "ipaddress": "$ipaddress",
    "port": "$port",
    "name": "$name",
    "weight": "$weight"
}
EOF

# No optional arguments for this request.

